{
    "name": "wiki-connect/wikibase-api",
    "type": "library",
    "description": "Wikibase API library",
    "keywords": ["MediaWiki", "Wikibase", "Wikidata", "API"],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Gerges Shamon",
            "email": "gerges.shamoon.dev@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "wiki-connect/mediawiki-api": "^1.0.7",
        "serialization/serialization": "~3.2||~4.0",
        "wikibase/data-model": "~9.2||~8.0",
        "wikibase/data-model-serialization": "~2.0",
        "wikibase/data-model-services": "~3.0"
    },
    "require-dev": {
        "mediawiki/mediawiki-codesniffer": "~38.0 || ~44.0",
        "data-values/common": "~0.4.0",
        "data-values/data-values": "~2.0",
        "data-values/geo": "~4.0",
        "data-values/number": "~0.10.0 || ~0.12.0",
        "data-values/serialization": "~1.0",
        "data-values/time": "~1.0",
        "data-values/validators": "~0.1.0 || ~1.0.0"
    },
    "autoload": {
        "psr-4": {
            "WikiConnect\\WikibaseApi\\": "src/",
            "Wikibase\\MediaInfo\\": "lib/wikibase/media-info/src/"
        }
    },
    "config": {
        "platform": {
            "php": "8.1.29"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
